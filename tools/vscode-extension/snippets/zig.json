  "ABI Initialization": {
    "prefix": "abi-init",
    "body": [
      "const std = @import(\"std\");",
      "const abi = @import(\"abi\");",
      "",
      "pub fn main() !void {",
      "    var gpa = std.heap.GeneralPurposeAllocator(.{}){};",
      "    defer _ = gpa.deinit();",
      "    const allocator = gpa.allocator();",
      "",
      "    // Initialize I/O backend (required for Zig 0.16)",
      "    var io_backend = std.Io.Threaded.init(allocator, .{",
      "        .environ = std.process.Environ.init(),",
      "    });",
      "    defer io_backend.deinit();",
      "    const io = io_backend.io();",
      "",
      "    // Initialize ABI framework",
      "    const config = abi.Config.init()",
      "        .withAI(${1:true})",
      "        .withGPU(${2:true})",
      "        .withDatabase(${3:true});",
      "",
      "    var framework = try abi.Framework.init(allocator, config);",
      "    defer framework.deinit();",
      "",
      "    ${0:// Your code here}",
      "}"
    ],
    "description": "Initialize ABI framework with full setup"
  },
